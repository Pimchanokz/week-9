#include<stdio.h>

void drawtube(int x)
{
	printf("||");
}

void drawdistance(int x)
{
	int b;
	for(b=1; b<=x; b++){
	printf("_");}
}

int main()
{
	int m,n;
	printf("the number of tubes (m) = ");
	scanf("%d",&m);
	printf("distance between tubes (n) = ");
	scanf("%d",&n);
	
	int a,b,x;
	a=1;
	while(a <= m) {
		drawtube(m);
		if (a<m){
		drawdistance(n);
		}
		a++;
	}

	return 0;
}
